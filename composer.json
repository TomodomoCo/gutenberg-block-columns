{
  "name": "tomodomo/block-columns",
  "description": "Variable-width columns block for Gutenberg",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomodomo",
      "email": "hello@tomodomo.co"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tomodomoco/wp-asset-registrar"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "installer-name": "tomodomo-block-columns"
  },
  "require": {
    "php": "^7.0",
    "tomodomo/wp-asset-registrar": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Tomodomo\\Gutenberg\\Block\\": [
        "lib/"
      ]
    }
  }
}
